{
  "name": "backend",
  "version": "1.0.0",
  
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "build:dev": "tsc --project tsconfig.dev.json",
    "build:minify": "tsc && npm run minify",
    "build:watch": "tsc --watch",
    "minify": "node minify.js",
    "obfuscate": "javascript-obfuscator dist --output dist --config obfuscator.config.js",
    "build:secure": "npm run build && npm run minify && npm run obfuscate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose"
  },
  "keywords": [
    "backend",
    "api",
    "express",
    "sequelize",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "description": "Backend API para o projeto Amigo",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.2",
    "sequelize": "^6.37.7",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.0",
    "javascript-obfuscator": "^4.1.0",
    "jest": "^29.5.0",
    "terser": "^5.28.1",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}